{
  "expo": {
    "name": "MyNotes",
    "slug": "MyNotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/notes_icon.png",
    "splash": {
      "image": "./assets/notes_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/notes_adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/notes_favicon.png"
    }
  }
}
